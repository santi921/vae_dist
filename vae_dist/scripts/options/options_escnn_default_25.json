{
    "architecture": {
        "channels": [256, 512, 1024],
        "kernel_size_in": [7, 7, 3],
        "stride_in":      [3, 3, 1],
        "kernel_size_out": [9, 5, 5],
        "stride_out":      [1, 2, 3],
        "max_pool": false, 
        "max_pool_kernel_size_in": 2, 
        "max_pool_loc_in": [2],
        "max_pool_kernel_size_out": 2, 
        "max_pool_loc_out": [2],
        "padding": [0],
        "padding_mode": "zeros"
    },
    "latent_dim": 2,
    "dilation": 1,
    "learning_rate": 0.001,
    "fully_connected_layers": [100, 100, 50],
    "log_wandb": true,
    "im_dim": 51,
    "dropout": 0.1,
    "batch_size": 32,
    "batch_norm": true,
    "bias": true,
    "initializer": "equi_var",
    "beta": 1.0,
    "reconstruction_loss": "mse",
    "lr_decay_factor": 0.5, 
    "lr_patience": 10, 
    "optimizer": "adam"
}