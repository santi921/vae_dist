{
    "architecture":{
        "channels": [128, 64, 64],
        "kernel_size_in": [9, 9, 3],
        "stride_in":      [1, 1, 3],
        "kernel_size_out": [9, 5, 5],
        "stride_out":      [1, 2, 3],
        "max_pool": false, 
        "max_pool_kernel_size_in": 2, 
        "max_pool_loc_in": [2],
        "max_pool_kernel_size_out": 2, 
        "max_pool_loc_out": [2],
        "padding_mode": "zeros",
        "padding": [0]
    },
    "latent_dim": 100,
    "dilation": 1,
    "learning_rate": 0.001,
    "fully_connected_layers": [500],
    "log_wandb": true,
    "im_dim": 21,
    "dropout": 0.1,
    "batch_size": 64,
    "batch_norm": true,
    "initializer": "equi_var",
    "bias": true,
    "beta": 1000.0,
    "reconstruction_loss": "mse",
    "lr_decay_factor": 0.5, 
    "lr_patience": 10, 
    "optimizer": "adam"
}